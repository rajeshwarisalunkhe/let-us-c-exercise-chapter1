/* 6 Write a program to copy the contents of one array into another in
reverse order */

#include <stdio.h>

int main(void)
{
	int i, j, number[5], number_list[5];

	for(i = 0;i < 5;i++)
	{
		printf("enter the number:");
		scanf("%d", &number[i]);
	}

	for(i = 0, j = 4;j >= 0;i++, j--)
	{
		number_list[j] = number[i];
	}

	for(j = 0;j < 5;j++)
	{
		printf("%d\t", number_list[j]);
	}

	printf("\n");

}
